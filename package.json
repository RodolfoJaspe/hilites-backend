{
  "name": "hilites-backend",
  "version": "1.0.0",
  "description": "Backend API for Hilites - Social Soccer Highlights App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fetch:matches": "node scripts/fetch-matches.js",
    "scheduler": "node scheduler.js",
    "production": "npx pm2-runtime start ecosystem.config.js --env production",
    "pm2:start": "npx pm2 start ecosystem.config.js",
    "pm2:stop": "npx pm2 stop ecosystem.config.js",
    "pm2:restart": "npx pm2 restart ecosystem.config.js",
    "pm2:logs": "npx pm2 logs",
    "pm2:monit": "npx pm2 monit"
  },
  "keywords": [
    "soccer",
    "highlights",
    "social",
    "api",
    "supabase"
  ],
  "author": "MyskJnz",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "langchain": "^0.3.35",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^6.0.13"
  }
}
